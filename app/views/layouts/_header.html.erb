<header>
	<%= link_to 'Home', root_path %>
	<%= link_to 'News', news_path %>
	<%= link_to 'Agenda', agenda_path %>
	<%= link_to 'A propos', presentation_path %> | 
	<% if signed_in? %>
		<%= link_to @current_user.name, edit_user_path(@current_user) %>
		<%= link_to "Sign out", signout_path, method: "delete" %>
	<% else %>
		<%= link_to 'Admin', login_path %>
	<% end %>
</header>